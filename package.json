{
  "name": "stikflat",
  "description": "glue your custom theme and content together",
  "version": "0.4.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/furudbat/stikflat.git"
  },
  "scripts": {
    "build": "browserify src/main.ts -p [ tsify ] -t [ envify --NODE_ENV production ] -t unassertify -t uglifyify -p common-shakeify | uglifyjs -cm > ./assets/js/app/bundle.min.js",
    "build-dev": "browserify src/main.ts --debug -p [ tsify ] -t [ envify --NODE_ENV development ] | exorcist ./assets/js/app/bundle.js.map > ./assets/js/app/bundle.js",
    "start": "bundle exec jekyll serve"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "bootstrap": "^4.5.2",
    "bootstrap-toggle": "^2.2.2",
    "brace": "^0.11.1",
    "clipboard": "^2.0.6",
    "esprima": "^4.0.1",
    "handlebars": "^4.7.6",
    "htmlhint": "^0.14.1",
    "jquery": "^3.5.1",
    "jquery-ui": "^1.12.1",
    "js-beautify": "^1.13.0",
    "js-yaml": "^3.14.0",
    "json-parse-better-errors": "^1.0.2",
    "jsonlint": "^1.6.3",
    "list.js": "^1.5.0",
    "localforage": "^1.9.0",
    "memory-cache": "^0.2.0",
    "mustache": "^4.0.1"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/ace": "0.0.43",
    "@types/bootstrap": "^4.5.0",
    "@types/bootstrap-toggle": "^2.2.1",
    "@types/clipboard": "^2.0.1",
    "@types/handlebars": "^4.1.0",
    "@types/htmlhint": "^0.9.1",
    "@types/jquery": "^3.5.1",
    "@types/jqueryui": "^1.12.13",
    "@types/js-beautify": "^1.11.0",
    "@types/js-yaml": "^3.12.5",
    "@types/json-parse-better-errors": "^1.0.0",
    "@types/list.js": "^1.5.3",
    "@types/memory-cache": "^0.2.1",
    "@types/mustache": "^4.0.1",
    "@types/node": "^14.6.1",
    "browser-pack-flat": "^3.4.2",
    "browserify": "^16.5.2",
    "common-shakeify": "^0.6.2",
    "envify": "^4.1.0",
    "exorcist": "^1.0.1",
    "ts-loader": "^8.0.3",
    "tsify": "^5.0.2",
    "typescript": "^4.0.2",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.10.2",
    "uglifyify": "^5.0.2",
    "unassertify": "^2.1.1"
  }
}
